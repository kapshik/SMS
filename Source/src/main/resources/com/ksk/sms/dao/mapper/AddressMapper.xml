<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ksk.sms.dao.mapper.AddressMapper">
    <select id="findOne" parameterType="String" resultType="com.ksk.sms.dao.domain.Address">
		SELECT * FROM tbl_address WHERE delete_flg = 0 AND zip =  #{zipcode}
    </select>
</mapper>
